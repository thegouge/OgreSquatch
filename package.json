{
  "name": "ogre-squatch",
  "version": "1.1.0",
  "description": "overwatch stat tracker based on the traversy media tutorial",
  "main": "server.js",
  "scripts": {
    "server": "nodemon server",
    "client": "npm run start --prefix client",
    "build": "yarn install",
    "start": "node server.js",
    "install:all": "yarn install && yarn install --prefix client",
    "dev": "concurrently \"yarn server\" \"yarn client\"",
    "predeploy": "yarn install:all",
    "deploy": "cd client && yarn deploy",
    "postdeploy": "git subtree push --prefix server origin railway-backend"
  },
  "author": "thegouge <thegouge42@gmail.com> (https://thegouge.github.io)",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.4"
  }
}
